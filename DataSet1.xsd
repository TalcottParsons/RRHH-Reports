                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
    Contacto.ContactoID, 
    Contacto.NombreCompleto, 
    Contacto.TelefonoMovil, 
    Contacto.Email, 
    Contacto.Departamento, 
    Contacto.Objetivo, 
    Contacto.Foto, 
    Competencias.CompetenciaID, 
    Competencias.Competencia, 
    Competencias.Dominio, 
    ExperienciaProfesional.ExperienciaID, 
    ExperienciaProfesional.Cargo, 
    ExperienciaProfesional.Institucion, 
    ExperienciaProfesional.AnioInicio, 
    ExperienciaProfesional.AnioFin, 
    FormacionAcademica.FormacionID, 
    FormacionAcademica.Institucion, 
    FormacionAcademica.Titulo, 
    FormacionAcademica.AnioInicio, 
    FormacionAcademica.AnioFin, 
    FormacionAcademica.Departamento, 
    Habilidades.HabilidadID, 
    Habilidades.Habilidad, 
    Referencias.ReferenciaID, 
    Referencias.Nombre, 
    Referencias.Telefono, 
    Referencias.TipoReferencia
FROM Contacto 
LEFT JOIN Competencias ON Contacto.ContactoID = Competencias.ContactoID 
LEFT JOIN ExperienciaProfesional ON Contacto.ContactoID = ExperienciaProfesional.ContactoID 
LEFT JOIN FormacionAcademica ON Contacto.ContactoID = FormacionAcademica.ContactoID 
LEFT JOIN Habilidades ON Contacto.ContactoID = Habilidades.ContactoID 
LEFT JOIN Referencias ON Contacto.ContactoID = Referencias.ContactoID
WHERE Contacto.ContactoID = @ContactoID</CommandText>
